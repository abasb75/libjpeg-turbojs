{
    "name": "@abasb75/turbojpeg-codec",
    "version": "3.1.3-b1",
    "description": "Libjpeg-turbo codec for js",
    "exports": {
         "./decode": {
            "require": "./dist/libjpegturb_inline_decoder.js",
            "import": "./dist/libjpegturb_es6_decoder.js"
        },
        "./encode": {
            "require": "./dist/libjpegturb_inline_encoder.js",
            "import": "./dist/libjpegturb_es6_encoder.js"
        },
        "./*": "./dist/*"
    },
    "files": [
        "dist"
    ],
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/abasb75/libjpeg-turbojs.git"
    },
    "bugs": {
        "url": "https://github.com/abasb75/libjpeg-turbojs/issues"
    },
    "homepage": "https://github.com/abasb75/libjpeg-turbojs#readme",
    "devDependencies": {
        "@tailwindcss/cli": "^4.1.18",
        "autoprefixer": "^10.4.22",
        "concurrently": "^9.2.1",
        "esbuild-copy-files": "^0.2.16",
        "fs": "^0.0.1-security",
        "live-server": "^1.2.2",
        "path": "^0.12.7",
        "postcss": "^8.5.6",
        "tailwindcss": "4.1",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    }
}
